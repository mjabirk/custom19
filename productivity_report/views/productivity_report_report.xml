<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="action_report_productivity" model="ir.actions.report">
            <field name="name">Employee Productivity Analysis</field>
            <field name="model">employee.productivity.analysis</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">productivity_report.report_empproductivityanalysis</field>
            <field name="report_file">productivity_report.report_empproductivityanalysis</field>
            <field name="binding_model_id" ref="model_employee_productivity_analysis"/>
            <field name="binding_type">report</field>
        </record>

        <record id="action_report_consumption" model="ir.actions.report">
            <field name="name">Raw Data Consumption</field>
            <field name="model">raw.data.consumption</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">productivity_report.report_raw_data_consumption</field>
            <field name="report_file">productivity_report.report_raw_data_consumption</field>
            <field name="binding_type">report</field>
        </record>

        <record id="action_delivery_summary_report" model="ir.actions.report">
            <field name="name">Delivery Summary (Alkon)</field>
            <field name="model">account.move</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">productivity_report.report_delivery_summary_document</field>
            <field name="report_file">productivity_report.report_delivery_summary_document</field>
            <field name="binding_model_id" ref="account.model_account_move"/>
            <field name="binding_type">report</field>
            <field name="print_report_name">'Delivery Summary - %s' % (object.name)</field>
        </record>

</odoo>
